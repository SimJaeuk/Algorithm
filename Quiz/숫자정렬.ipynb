   "source": [
    "# 입력받을 숫자 N 입력받기\n",
    "n = int(input())\n",
    "\n",
    "# 입력 받은 정수 담을 빈 리스트 생성\n",
    "array = []\n",
    "\n",
    "# 입력받은 숫자 n의 길이만큼 반복하여 리스트에 담기\n",
    "for i in range(n):\n",
    "    array.append(int(input()))\n",
    "\n",
    "# 내장 정렬 라이브러리를 이용하여 내림차순 정렬\n",
    "array = sorted(array, reverse=True)\n",
    "\n",
    "# 결과 붙여서 출력\n",
    "for i in array:\n",
    "    print(i, end=' ')"
   ]
  }
